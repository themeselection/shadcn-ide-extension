{
  "name": "flyonui-vscode-extension",
  "displayName": "FlyonUI IDE Extension",
  "private": true,
  "description": "Edit Your UI With a Prompt.",
  "version": "1.0.3",
  "publisher": "themeselection",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.85.0"
  },
  "license": "AGPL-3.0-only",
  "categories": [
    "AI",
    "Debuggers",
    "Machine Learning",
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onUri"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "flyonui.setAgent",
        "title": "Set the preferred agent you want to use with FlyonUI",
        "category": "FlyonUI"
      },
      {
        "command": "flyonui.openDataPanel",
        "title": "Open FlyonUI Blocks Panel",
        "category": "FlyonUI"
      },
      {
        "command": "flyonui.focusApiDataView",
        "title": "Focus FlyonUI Blocks Panel",
        "category": "FlyonUI"
      },
      {
        "command": "shadcn.openBlocksPanel",
        "title": "Open shadcn/ui Blocks Panel",
        "category": "shadcn"
      },
      {
        "command": "shadcn.focusBlocksView",
        "title": "Focus shadcn/ui Blocks Panel",
        "category": "shadcn"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "flyonui-sidebar",
          "title": "FlyonUI Extension",
          "icon": "./icon.svg"
        }
      ]
    },
    "views": {
      "flyonui-sidebar": [
        {
          "type": "webview",
          "id": "flyonui.apiDataView",
          "name": "Premium FlyonUI Blocks",
          "icon": "$(rocket)",
          "when": "true"
        },
        {
          "type": "webview",
          "id": "shadcn.blocksView",
          "name": "shadcn/ui Blocks",
          "icon": "$(code)",
          "when": "true"
        }
      ]
    },
    "icons": {
      "flyonui-icon": {
        "description": "FlyonUI icon",
        "default": {
          "fontPath": "./stagewise-icons.woff",
          "fontCharacter": "\\E800"
        }
      }
    },
    "configuration": [
      {
        "title": "FlyonUI",
        "type": "object",
        "properties": {
          "flyonui.telemetry.enabled": {
            "type": "boolean",
            "default": true,
            "description": "Enable anonymous usage data collection to help improve the product. No personal data is collected.",
            "tags": [
              "telemetry",
              "usesOnlineServices"
            ],
            "markdownDescription": "Enable anonymous usage data collection to help improve the product. No personal data is collected.\n\nRead more about our [telemetry data collection](https://github.com/themeselection/flyonui-extension/blob/main/apps/vscode-extension/TELEMETRY.md)."
          }
        }
      },
      {
        "title": "FlyonUI",
        "type": "object",
        "properties": {
          "flyonui.licenseKey": {
            "type": "string",
            "default": "",
            "description": "Your FlyonUI Pro license key. Get your license at https://flyonui.com/pro",
            "markdownDescription": "Your FlyonUI Pro license key. Get your license at [FlyonUI Pro](https://flyonui.com/pro)"
          }
        }
      },
      {
        "title": "shadcn",
        "type": "object",
        "properties": {
          "shadcn.licenseEmail": {
            "type": "string",
            "default": "",
            "description": "Your email associated with the license"
          },
          "shadcn.licenseKey": {
            "type": "string",
            "default": "",
            "description": "Your license key"
          }
        }
      }
    ]
  },
  "homepage": "https://flyonui.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/themeselection/flyonui-extension.git"
  },
  "bugs": {
    "url": "https://github.com/themeselection/flyonui-extension/issues"
  },
  "keywords": [
    "flyonui",
    "ui",
    "components",
    "tailwindcss",
    "extension",
    "vscode",
    "stagewise-fork"
  ],
  "scripts": {
    "vscode:prepublish": "pnpm run build",
    "run-server": "node ./out/server.js",
    "build": "webpack --mode production --config webpack.config.js"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.15.2",
    "@types/vscode": "^1.85.0",
    "@types/ws": "^8.18.1",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "@vscode/vsce": "^3.3.2",
    "@yarnpkg/lockfile": "^1.1.0",
    "copy-webpack-plugin": "^13.0.1",
    "dotenv": "^17.2.0",
    "ovsx": "^0.10.5",
    "ts-loader": "^9.5.2",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "vitest": "3.1.2",
    "webpack": "^5.100.0",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "1.15.1",
    "@stagewise/agent-client": "workspace:*",
    "@stagewise/agent-interface": "workspace:*",
    "@stagewise/agent-runtime-vscode": "workspace:*",
    "@stagewise/extension-toolbar-srpc-contract": "workspace:*",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.1",
    "@types/js-yaml": "^4.0.9",
    "@types/yarnpkg__lockfile": "^1.1.9",
    "axios": "^1.6.7",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "js-yaml": "^4.1.0",
    "posthog-node": "^4.17.1",
    "ws": "^8.18.3",
    "zod": "3.25.76"
  },
  "turbo": {
    "tasks": {
      "build": {
        "dependsOn": [
          "^build"
        ],
        "outputs": [
          "out/**"
        ]
      }
    }
  }
}
